{
  "expo": {
    "name": "beanbucket",
    "slug": "BeanBucket",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
        
      },
      "package": "com.codyle.beanbucket",
      "versionCode": 7,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  
  }
}
